# -*- coding: utf-8 -*-
import scrapy


class CarenginesSpider(scrapy.Spider):
    name = 'carEngines'
    allowed_domains = ['cars-data.com']
    start_urls = ['http://cars-data.com/en/audi-a5-sportback-1.4-tfsi-150hp-specs/%d' % x for x in xrange(1,100)]

    def parse(self, response):
        header = response.css("h1::text").extract_first()
        print(header)
        pass
